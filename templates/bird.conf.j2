{% macro print_config(config) %}
{% if config is string %}
{{ config }}
{%- elif config is mapping %}
{% for key, value in config.items() %}
{{ key }} {
    {{ print_config(value) | indent }}}
{%- endfor %}{# for key, value in config.items() #}
{% else %}
{% for element in config %}
{{ print_config(element) }};
{% endfor %}
{% endif %}
{% endmacro -%}

